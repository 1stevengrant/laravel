{
    "name": "auth0/laravel-php-web-app",
    "type": "project",
    "description": "Demonstration of the Auth0 Laravel SDK",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "auth0/login": "^7.0",
        "fruitcake/laravel-cors": "^3.0.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "nunomaduro/larastan": "^1.0",
        "nyholm/psr7": "^1.4",
        "orchestra/testbench": "^7.0",
        "phpstan/phpstan": "^1.7",
        "phpstan/phpstan-strict-rules": "1.4.3",
        "psalm/plugin-laravel": "^2.0",
        "rector/rector": "^0.13.6",
        "vimeo/psalm": "^4.28",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "tests": [
            "@tests:phpinsights",
            "@tests:phpstan",
            "@tests:pest"
        ],
        "tests:phpinsights": [
            "Composer\\Config::disableProcessTimeout",
            "@php ./vendor/bin/phpinsights -v --no-interaction"
        ],
        "tests:phpstan": [
            "Composer\\Config::disableProcessTimeout",
            "@php ./vendor/bin/phpstan analyse --ansi --memory-limit 512M"
        ],
        "tests:pest": [
            "Composer\\Config::disableProcessTimeout",
            "@php ./vendor/bin/pest --stop-on-failure --coverage"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "composer.local.json"
            ],
            "recurse": true,
            "replace": true,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true,
            "wikimedia/composer-merge-plugin": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
